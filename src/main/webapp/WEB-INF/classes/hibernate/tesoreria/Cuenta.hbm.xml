<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.infoCofrade.tesoreria.cuenta.vo.CuentaVO" table='"CUENTA"'>
		<id column="id" name="id" type="java.lang.Long">
			<generator class="identity" />
		</id>
		<property column='"idCuentaPadre"' name="idCuentaPadre" type="java.lang.Long" />
		<property column='"numeroCuenta"' name="numeroCuenta" type="java.lang.Integer" />
		<property column='"nombre"' name="nombre" type="java.lang.String" length="255" />
		<property column='"idTipoActividad"' name="idTipoActividad" type="java.lang.Long" />
		<property column='"esConceptoFijo"' name="esConceptoFijo" type="java.lang.Boolean" />
		<property column='"anyo"' name="anyo" type="java.lang.Integer" />
		<property column='"nivel"' name="nivel" type="java.lang.Integer" />
	</class>
</hibernate-mapping>