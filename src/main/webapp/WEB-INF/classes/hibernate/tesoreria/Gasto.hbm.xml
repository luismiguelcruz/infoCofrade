<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.infoCofrade.tesoreria.gasto.vo.GastoVO" table='"GASTO"'>
		<id column="id" name="id" type="java.lang.Long">
			<generator class="identity" />
		</id>
		<property column='"idCuenta"' name="idCuenta" type="java.lang.Long" not-null="true" />
		<property column='"cuantia"' name="cuantia" type="java.lang.Double" />
		<property column='"concepto"' name="concepto" type="java.lang.String" />
		<property column='"anyo"' name="anyo" type="java.lang.Integer" />
	</class>
</hibernate-mapping>