<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.infoCofrade.mayordomia.cuotaHermano.vo.CuotaHermanoVO" table='"CUOTA_HERMANO"'>
		<id column="id" name="id" type="java.lang.Long">
			<generator class="identity" />
		</id>
		<property column='"cuantia"' name="cuantia" type="java.lang.Double" not-null="true" />
		<property column='"edadMinima"' name="edadMinima" type="java.lang.Integer" not-null="true" />
		<property column='"edadMaxima"' name="edadMaxima" type="java.lang.Integer" not-null="true" />
		<property column='"idCuenta"' name="idCuenta" type="java.lang.Long" />
	</class>
</hibernate-mapping>