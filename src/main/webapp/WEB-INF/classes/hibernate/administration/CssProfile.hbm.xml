<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.infoCofrade.administration.cssProfile.vo.CssProfileVO" table='"CSS_PROFILE"'>
		<id column="id" name="id" type="java.lang.Long">
			<generator class="identity" />
		</id>
		<property column='"color"' name="color" type="java.lang.String" length="10" not-null="true"/>
		<property column='"urlPath"' name="urlPath" type="java.lang.String" length="350" not-null="true"/>
	</class>
</hibernate-mapping>