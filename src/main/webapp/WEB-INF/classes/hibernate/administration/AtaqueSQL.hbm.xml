<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.infoCofrade.administration.ataqueSQL.vo.AtaqueSQLVO" table='"ATAQUESQL"'>
		<id column="id" name="id" type="java.lang.Long">
			<generator class="identity" />
		</id>
		<property column='"ip"' name="ip" type="java.lang.String" />
		<property column='"sentencia"' name="sentencia" type="java.lang.String" />
		<property column='"hora"' name="hora" type="java.util.Date" />
	</class>
</hibernate-mapping>