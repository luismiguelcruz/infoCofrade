<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.infoCofrade.administration.provincia.vo.ProvinciaVO" table='"PROVINCIA"'>
		<id column="id" name="id" type="java.lang.Long">
			<generator class="identity" />
		</id>
		<property column='"provincia"' name="provincia" type="java.lang.String" length="255" not-null="true"/>
		<property column='"provinciaSeo"' name="provinciaSeo" type="java.lang.String" length="255" />
		<property column='"codigoProvincia"' name="codigoProvincia" type="java.lang.String" length="3" />
		<property column='"idPais"' name="idPais" type="java.lang.Long" />
	</class>
</hibernate-mapping>