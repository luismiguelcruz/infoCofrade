<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.infoCofrade.administration.secretaria.calleView.vo.CalleViewVO" table='"CALLE_VIEW"'>
		<id column="id" name="id" type="java.lang.Long" />
		<property column='"idTipoVia"' name="idTipoVia" type="java.lang.Long" not-null="true"/>
		<property column='"tipoVia"' name="tipoVia" type="java.lang.String" />
		<property column='"nombreVia"' name="nombreVia" type="java.lang.String" length="300" />
		<property column='"numKmMinimo"' name="numKmMinimo" type="java.lang.String" length="5" />
		<property column='"numKmMaximo"' name="numKmMaximo" type="java.lang.String" length="5" />
		<property column='"idLocalidad"' name="idLocalidad" type="java.lang.Long" />
		<property column='"idSector"' name="idSector" type="java.lang.Long" />
		<property column='"encargado"' name="encargado" type="java.lang.String" length="300" />
		<property column='"localidad"' name="localidad" type="java.lang.String" length="255" />
		<property column='"codigoPostal"' name="codigoPostal" type="java.lang.Integer" />
		<property column='"latitud"' name="latitud" type="java.lang.Double" />
		<property column='"longitud"' name="longitud" type="java.lang.Double" />
		<property column='"idProvincia"' name="idProvincia" type="java.lang.Long" />
		<property column='"provincia"' name="provincia" type="java.lang.String" length="255" />
		<property column='"idPais"' name="idPais" type="java.lang.Long" />
		<property column='"pais"' name="pais" type="java.lang.String" length="255" />
	</class>
</hibernate-mapping>